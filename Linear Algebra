from numpy import array , matmul , linalg
my_matrix = array ([ [1, 2, 5, 6],
                        [4, -4,-6, 8],
                        [-12, 1, 3, 9],
                        [18 , 0, 0, 6]])
my_vector = array([1, 6, 7 , 2])
solution = linalg.solve( my_matrix , my_vector )
print ("Solution is x , y , z , t =", solution )
